{
  "name": "interpolated-colors",
  "version": "3.0.0",
  "description": "A tool to create dynamic colors by interpolating multiple colors",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/interpolated-colors#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/interpolated-colors/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/interpolated-colors.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup --format cjs,esm src/mod.ts --dts src/mod.ts",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "color": "^4.2.3"
  },
  "devDependencies": {
    "@types/color": "^3.0.3",
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.0",
    "np": "^7.6.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "tslib": "^2.4.0",
    "tsup": "^6.1.3",
    "typescript": "^4.6.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
